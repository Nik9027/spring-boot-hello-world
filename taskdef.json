{
    "family": "helloworld",
    "containerDefinitions": [
        {
            "image": "124864831979.dkr.ecr.us-west-2.amazonaws.com/hello-world:v_%BUILD_NUMBER%",
            "name": "helloworld",
            "cpu": 10,
            "memory": 256,
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 8080,
                    "hostPort": 8080
                }
            ]
        }
    ]
}
